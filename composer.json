{
    "name": "orizaba/laravel-components",
    "description": "Reusable components for Laravel",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Orizaba\\LaravelComponents\\": "src/"
        },
        "files": [
            "src/Support/helpers/functions.php"
        ]
    },
    "authors": [
        {
            "name": "Page Carbajal",
            "email": "awesome@pagecarbajal.com"
        }
    ],
    "require": {
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0",
        "illuminate/routing": "^9.0"
    },
    "require-dev": {
        "illuminate/contracts": "^9.27"
    },
    "autoload-dev": {
        "files": [
            "src/Support/helpers/laravel-polyfill.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orizaba\\LaravelComponents\\OrizabaServiceProvider"
            ]
        }
    },
    "extra_extra": {
        "aliases": {
            "Debugbar": "Barryvdh\\Debugbar\\Facade"
        }
    }
}
